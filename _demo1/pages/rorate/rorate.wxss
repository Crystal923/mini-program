.rorate_container {
  width: 100%;
  height: 100%;
}

.rorate_container .rorate_img {
  width: 72rpx;
  height: 72rpx;
  display: block;
  margin: 20rpx auto;
  animation: gogogo 1.5s infinite linear;
}

@keyframes gogogo {
  0% {
    -webkit-transform: rotate(0deg);
  }

  50% {
    -webkit-transform: rotate(180deg);
  }

  100% {
    -webkit-transform: rotate(360deg);
  }
}
